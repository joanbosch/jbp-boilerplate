    "next": "14.2.4",
    "next-auth": "^5.0.0-beta.19",
    "next-contentlayer2": "^0.5.0",
    "next-plausible": "^3.12.0",
    "next-themes": "^0.3.0",
    "nodemailer": "^6.9.14",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.1",
    "react-tweet": "^3.2.1",
    "resend": "^3.4.0",
    "schema-dts": "^1.1.2",
