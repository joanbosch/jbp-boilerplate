    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-pretty-code": "^0.9.11",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.3",
    "remark-code-import": "^1.2.0",
    "remark-gfm": "^4.0.0",
    "rimraf": "^5.0.7",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.11.0",
